{
    "name": "adorador/shippingstation",
    "description": "This a Shipping station api wrapper.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Drew Adorador",
            "email": "acadorador@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "illuminate/support": "5.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "orchestra/testbench": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "ShippingStation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShippingStation\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShippingStation\\ShippingStationProvider"
            ],
            "aliases": {
                "ShippingStation": "ShippingStation\\ShippingStationFacade"
            }
        }
    }
}
